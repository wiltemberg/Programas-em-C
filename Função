#include <stdio.h>
#include <stdlib.h>
void maior(int n1,int n2,int n3,int n4,int n5);
main ()
{
    int x1,x2,x3,x4,x5;
    printf ("\nDIGITE CINCO NUMEROS INTEIROS.\n");
    scanf ("%d %d %d %d %d",&x1,&x2,&x3,&x4,&x5);
    maior(x1,x2,x3,x4,x5);
    return 0;
    system("pause");
}
void maior(int n1,int n2,int n3,int n4,int n5)
{
    int aux;
    if (n1>n2)
    aux=n1;
    else
    aux=n2;
    if (n3>aux)
    aux=n3;
    if (n4>aux)
    aux=n4;
    if (n5>aux)
    aux=n5;
    printf ("\nO MAIOR NUMERO DIGITADO FOI: %d .",aux);
    if (n1<n2)
    aux=n1;
    else
    aux=n2;
    if (n3<aux)
    aux=n3;
    if (n4<aux)
    aux=n4;
    if (n5<aux)
    aux=n5;
    printf ("\nO MENOR NUMERO DIGITADO FOI: %d .",aux);
    system("pause");
}
